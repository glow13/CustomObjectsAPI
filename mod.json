{
    "geode": "5.0.0-alpha.2",
    "gd": {
        "win": "2.2081",
        "android": "2.2081",
        "mac": "2.2081",
        "ios": "2.2081"
    },
    "id": "glow12.custom-objects-api",
    "name": "Custom Objects API",
    "version": "v1.0.0",
    "developer": "glow12",
    "description": "Add custom objects to Geometry Dash!",
    "dependencies": {
        "alphalaneous.editortab_api": ">=v2.0.0-alpha.2"
    },
    "api": {
        "include": [
            "include/CustomObjectsAPI.hpp",
            "include/CustomObjectBase.hpp",
            "include/data/CustomSpriteConfig.hpp",
            "include/data/CustomObjectConfig.hpp",
            "include/data/CustomObjectsMod.hpp",
            "include/object/*.hpp"
        ]
    },
    "resources": {
        "sprites": [
            "resources/*.png"
        ],
        "spritesheets": {
            "CustomObjects": [
                "resources/sprites/*.png"
            ],
            "cat": [
                "resources/cat/*.png"
            ]
        }
    },
    "settings": {
        "debug": {
            "type": "title",
            "name": "Debug"
        },
        "force-generation": {
            "type": "bool",
            "name": "Force Spritesheet Generation",
            "description": "Always generate the spritesheets on game launch, used for debugging",
            "default": false
        },
        "disable-aa": {
            "type": "bool",
            "name": "Disable Anti-Aliasing",
            "description": "Disables the anti-aliasing effect applied to the custom objects spritesheet",
            "default": false
        }
    }
}
